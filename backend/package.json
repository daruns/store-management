{
  "name": "nest-knex",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "engines": {
    "node": "12.22.6"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "knex": "knex --knexfile ./src/database/knex.ts",
    "migrate:latest": "yarn run knex migrate:latest",
    "migrate:make": "yarn run knex migrate:make -x ts",
    "migrate:rollback": "yarn run knex migrate:rollback",
    "seed:run": "yarn run knex seed:run",
    "seed:make": "yarn run knex seed:make"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/platform-socket.io": "^6.11.11",
    "@nestjs/platform-ws": "^8.2.3",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/websockets": "^6.11.11",
    "@types/dotenv": "8.2.0",
    "@types/express-serve-static-core": "^4.17.25",
    "@types/multer": "^1.4.7",
    "@types/qs": "6.9.6",
    "aws-sdk": "^2.1037.0",
    "bcrypt": "^5.0.1",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "cloudinary": "^1.27.1",
    "dotenv": "8.2.0",
    "express-rate-limit": "^5.5.0",
    "global": "^4.4.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "knex": "0.20.13",
    "knex-mariadb": "^2.0.0",
    "libphonenumber-js": "^1.9.37",
    "lodash": "4.17.15",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "mysql": "^2.18.1",
    "mysql2": "2.1.0",
    "nodemailer": "^6.7.0",
    "objection": "2.1.3",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^24.0.18",
    "@types/knex": "0.16.1",
    "@types/node": "^12.7.5",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.8",
    "faker": "4.1.0",
    "husky": "4.2.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "socket.io": "^4.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts, js,jsx}": [
      "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "tslint --fix \"src/**/*.ts\" \"test/**/*.ts\" -t verbose",
      "tslint -p tsconfig.json -c tslint.json",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
